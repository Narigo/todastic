{
  "name": "todastic",
  "version": "1.0.0",
  "description": "Simple self-, project- and task management.",
  "main": "dist/index.js",
  "scripts": {
    "build": "run-s build:*",
    "build:client": "webpack --config config/webpack/client.js",
    "build:server": "webpack --config config/webpack/server.js",
    "build:storybook": "cross-env NODE_ENV=production BABEL_ENV=development build-storybook -c config/storybook -o ./dist/storybook",
    "clean": "rimraf dist/",
    "dev": "cross-env NODE_ENV=development run-p dev:*",
    "dev:client": "npm run build:client -- --watch",
    "dev:server": "npm run build:server -- --watch",
    "dev:run": "sleep 5 && nodemon --watch 'dist/server/index.js' --exec 'node dist/server/index.js'",
    "start": "run-s clean build start:server",
    "start:server": "node dist/server/index.js",
    "storybook": "cross-env NODE_ENV=development BABEL_ENV=development start-storybook -p 6006 -c config/storybook",
    "takeoff": "takeoff",
    "test": "run-s test:**",
    "test:lint:eslint": "eslint --ext .js,.ts,.tsx --max-warnings 0 src config",
    "test:lint:stylelint": "stylelint 'src/**/*.scss' --config config/.stylelintrc",
    "test:unit:client": "jest --config config/jest/client-unit.config.js",
    "test:unit:server": "jest --config config/jest/server-unit.config.js",
    "test:storyshots": "jest --config config/jest/storyshots.config.js --env=jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/compose-us/todastic.git"
  },
  "keywords": [],
  "author": "compose.us GmbH <hello@compose.us>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/compose-us/todastic-2/issues"
  },
  "homepage": "https://github.com/compose-us/todastic-2#readme",
  "dependencies": {
    "@babel/runtime": "^7.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
